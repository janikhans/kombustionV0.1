<% provide(:title, "OEM Vechicles") %>
<div class="row">
<div class="col-lg-12">
    <h1 class="page-header">OEM Vehicles</h1>
</div>
<!-- /.col-lg-12 -->
</div>

<row>
<div class="col-md-6">
      <%= link_to "Add OEM Vehicle", new_vehicle_path, class: "btn btn-primary" %>
</div>
</row>

  
<table id = "Vehicles" class="table table-striped table-bordered">
<thead>
  <tr>
    <th>Make</th>
    <th>Model</th>
    <th>Year</th>
    <th>Category</th>
    <th>Edit Vehicle</th>
    <th>See More</th>
  </tr>
</thead>
<tbody>
  <% @vehicles.each do |vehicle| %>
    <tr>
      <td><%= vehicle.make %></td>    
      <td><%= vehicle.model %></td>
      <td><%= vehicle.year %></td>
      <td><%= vehicle.category %></td>
      <td><%= link_to  "Edit", edit_vehicle_path(vehicle.id) %></td>
      <td><%= link_to  "Show", vehicle_path(vehicle.id) %></td>
    </tr>
  <% end %>
 </tbody>
</table>