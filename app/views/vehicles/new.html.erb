<% provide(:title, 'Add Vehicle') %>
<div class="col-lg-12">
    <h1 class="page-header">New Vehicle</h1>
</div>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@vehicle) do |f| %>
        <%= render 'fields', f: f %>
      <%= f.submit "Add Vehicle", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>